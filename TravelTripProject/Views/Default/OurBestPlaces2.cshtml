@using TravelTripProject.Models.Entities
@model List<Blog>

<div class="box1-left">
    @foreach (var item in Model)
    {
        <div class="bot-img">
            <img src="@item.ImageURL" alt="" />
        </div>
        <div class="bot-desc1">
            <p>@item.Description.Substring(0, 75)...</p>
        </div>
        <div class="clear"></div>
    }
</div>